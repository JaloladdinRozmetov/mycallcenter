APP_NAME="My Call Center"
APP_ENV=develop
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

LOG_CHANNEL=stack
LOG_STACK=single,loki
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=call_center_dev
DB_USERNAME=call_center
DB_PASSWORD=call_center

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

FILESYSTEM_DISK=public
QUEUE_CONNECTION=database

CACHE_STORE=database

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

ASTERISK_ARI_BASE_URI=http://asterisk:8088/ari
ASTERISK_ARI_USERNAME=ari
ASTERISK_ARI_PASSWORD=secret
ASTERISK_ARI_APP=outbound
ASTERISK_ARI_OUTBOUND_ENDPOINT=PJSIP/provider
ASTERISK_ARI_CALLER_ID="Call Center"
ASTERISK_ARI_DIAL_TIMEOUT=20

TTS_ENDPOINT=http://tts:5002
TTS_API_KEY=
TTS_VOICE=default
TTS_LANGUAGE=en-US
TTS_FORMAT=wav
TTS_TIMEOUT=15

LOKI_ENDPOINT=http://loki:3100
LOKI_BASIC_AUTH=
LOKI_TENANT_ID=

VITE_APP_NAME="${APP_NAME}"
